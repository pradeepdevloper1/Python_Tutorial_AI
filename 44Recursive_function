#Recursive Function :Factorial of a number
def factorial(num):
    """ this function Returns Factorial of number 
    """
    return num if num<=1 else (num * factorial(num-1))

num=5
print("Factorial of {0} is : {1}".format(num,factorial(num)))



#Recursive Function:Fibbonacci Series
def fibonacci(num):
    """ this function Returns Factorial of number 
    """
    return num if num<=1 else (fibonacci(num-1)+fibonacci(num-2))

nterms=10
print("Factorial sequence")
for num in range(nterms):
    print(fibonacci(num))
